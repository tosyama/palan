ccall int32 printf();

int32 i, j, k = 1, 2;

i, j -> j, i;
printf("%d %d\n", i, j);

int32[3] a, b = arr(0), arr(3);

b, a[1] -> a, i;

printf("%d %d\n", a[1], i);

arr(0) -> a;

a, b -> b, a;
printf("%d %d\n", a[1], b[1]);

arr(0) -> a;
arr(3) -> b;
a[1], (9->a[1]) -> b[2], a[2];
printf("%d %d %d\n", a[1],a[2],b[2]);

int32[2][3] aa, bb;
arr(0) -> aa[1];
arr(3) -> bb[0];

aa[1], bb[0] -> bb[0], aa[1];

printf("%d %d\n", aa[1][1], bb[0][1]);

func arr(int32 base) -> int32[3] a
{
	int32 i = 0;
	while (i < 3) {
		base + i -> a[i];
		i+1 -> i;
	}
}
