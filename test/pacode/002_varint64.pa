ccall int32 printf();
ccall void exit();

int64 aa, int64 ab add(int64 a, int64 b)
{
	aa,ab = a+1,b+2;
}

void main()
{
	int64 a = 1;
	int64 b,c,d = 2,3,4;
	int64 e,f = -1,-2;
	int64 h;
	int64 i,j = 0, 1-4;
	{
		int64 k = 3;
		e = -k;
	}
	h = a+d;
	f = e-f+-d;
	int64 k,l = add(5,6);
	printf("%d %d %d", h, j, f);
	printf(" %d %d", k, l);
	printf("\n");
	exit(0);	// to flush buffer.
}
