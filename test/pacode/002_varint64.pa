ccall int64 printf();
ccall exit();

func int64 aa, int64 ab
	add(int64 a, int64 b)
{
	aa,ab = a+1,b+2;
}

func main()
{
	int64 a = 1;
	int64 b,c,d = 2,3,4;
	int64 e,f = -1,-2;
	int64 h;
	int64 i,j = 0, 1-4;
	{
		int64 k = 3;
		e = -k;
	}
	h = a+d;
	f = e-f+-d;
	int64 k,l = add(5,6);
	printf("%d %d %d %d %d %d\n", h, j, f, a+b+c, k, l);
	exit(0);	// to flush buffer.
}
