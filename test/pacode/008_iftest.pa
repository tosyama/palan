ccall int32 printf();
ccall exit();

func elif(int32 i)
{
	if i == 0 {
		printf("[if]");
	} else if i == 1 {
		printf("[elif]");
	} else {
		printf("[else]");
	}
}

elif(0);
elif(1);
elif(2);

if 1 {
	printf("[true]");
}

if 0 {
	printf("[false]");
}

int64 i = 3;
int32 b = 2;
if i==b==0 {
	printf("%d", b);
}

exit(0);
