ccall int32 printf();
ccall exit();

func elif(int32 i)
{
	if i == 0 {
		printf("[if]");
	} else if i == 1 {
		printf("[elif]");
	} else {
		printf("[else]");
	}
}

elif(0);
elif(1);
elif(2);

if 1 {
	printf("[true]");
}

if 0 {
	printf("[false]");
}

printf("\n");

int32 i = 0;
while i < 3 {
	printf("%d", i);

	if i == 1 { printf("=="); }
	if i != 1 { printf("!="); }
	if i < 1 { printf("<"); }
	if i > 1 { printf(">"); }
	if i <= 1 { printf("<="); }
	if i >= 1 { printf(">="); }

	i+1 -> i;
}

printf("\n");
sbyte sb = -1;
byte b = 255;

if sb > 1 { printf("[ss]"); }
if b > 1 { printf("[us]"); }
if b > 1u { printf("[uu]"); }

exit(0);
