ccall printf(...);
ccall setenv(...);
ccall mtrace();
ccall muntrace();

setenv("MALLOC_TRACE", "out/mtrace030-1", 1);

int32 i1,i2,i3,i4,i5,i6;
byte b1,b2,b3;

mtrace();
{
	@[3]int32 arr1 = [1,2,3];
	arr1[1] -> i1;

	[2]@[3]int32 arr2 = [1,2,3][4,5,6];
	arr2[1][1] -> i2;

	[3]int32 arr3 = arr1;
	arr3[2] -> i3;

	[2][3]int32 aaa = arr2;

	[1][2]@[3]int32 x1 = [[1,2,3][4,5,6]];
	[1][2][3]int32 x2 = x1;
	x2[0][1][1] -> i4;
	9 -> x2[0][0][1];	// check writable.

	@[3]int32 xx = x2[0][0];
	xx[1] -> i5;
	refread(x1[0][1], 1) -> i6;

	@[10]byte str = "test";
	[2]@[10]byte strs = ["test1", "test2"];
	[2]@[?]byte strs2 = ["test3", "test4"];
	
	str[2] -> b1;
	strs[1][4] -> b2;
	strs2[0][4] -> b3;
}
muntrace();

printf("%d %d %d %d %d %d %c%c%c\n", i1, i2, i3, i4, i5, i6, b1,b2,b3);

type A {
	int16 i;
	@[3]int32 arr;
};

type B {
	byte b1;
	byte b2;
	flo64 d;
	flo32 f;
};

flo64 d, flo32 f;

setenv("MALLOC_TRACE", "out/mtrace030-2", 1);
mtrace();
{
	A a = [3, [1,2,3]];
	@A b = a;
	@[3]int32 aarr = b.arr;
	aarr[1] -> i1;

	@B bb = [1,2,1.23,2.34];
	bb.b2 -> b2;
	bb.d -> d;
	bb.f -> f;
}
muntrace();

printf("%d %d %.2f %.2f", i1, b2, d, f);

func refread(@[3]int32 arr, int32 x) -> int32 i
{
	arr[x] -> i;
}
