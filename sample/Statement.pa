for (int i=0; i<10; ++i)
	print(i.str);

int i;
while (i<10) {
	print(i.str);
	++i;
}

// break / continue
for (int i=0; i<10; ++i) :out
	for (int j=0; j<10; ++j) {
		if (i==8 && j==5) break out;
		print(i.str "," j.str "\n");
	}

int i;
while (i<10) :out
{
	++i;
	for (int j=0; j<10; ++j)
		if (i==8 && j==5) continue out ;
}

for(int i:1..5) print(i.str);
for(int i:1<<5) print(i.str);

int arr[10] = (1..10);
for(int i: arr) print(i.str);

