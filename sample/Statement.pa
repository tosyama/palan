for int i=0; i<10; ++i {
	print(i.str);
}

int i;
while i<10 {
	print(i.str);
	++i;
}

// break / continue
for@out int i=0; i<10; ++i {
	for int j=0; j<10; ++j {
		if (i==8 && j==5) break out;
		print(i.str "," j.str "\n");
	}
}

int i;
while@out i<10
{
	++i;
	for int j=0; j<10; ++j {
		if i==8 && j==5 { continue out ; }
	}
}

// forで :の場合は型省略(上位とかぶる場合は警告)
for i:1<<5 { print(i.str); }

int arr[10] = (1..10);
for i: arr { print(i.str); }

