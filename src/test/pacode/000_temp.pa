func set_arrarr([3][4]int32 >>a) -> [3][4]int32 a
{
	int32 i, j;

	0 -> i;
	while i<3 {
		0 -> j;
		while j<4 {
			i*4+j -> a[i][j];
			j+1 -> j;
		}
		i+1 -> i;
	}
}

